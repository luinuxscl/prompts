{
    "name": "luinuxscl/prompts",
    "description": "Un paquete Laravel minimalista para gestionar y anidar prompts de texto",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LuinuxSCL",
            "email": "info@luinuxscl.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Luinuxscl\\Prompts\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luinuxscl\\Prompts\\PromptsServiceProvider"
            ],
            "aliases": {
                "Prompts": "Luinuxscl\\Prompts\\Facades\\Prompts"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
